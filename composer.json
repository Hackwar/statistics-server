{
    "name": "joomla/jstats-server",
    "type": "project",
    "description": "Joomla Stats Collection Server",
    "keywords": ["joomla"],
    "homepage": "http://github.com/joomla-extensions/jstats-server",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Don Gilbert",
            "email": "don@dongilbert.net",
            "homepage": "http://dongilbert.net/"
        }
    ],
    "require": {
        "php": ">=5.6",
        "joomla/application": "~1.6",
        "joomla/controller": "~1.1",
        "joomla/database": "~1.2",
        "joomla/di": "~1.3",
        "joomla/github": "~1.3",
        "joomla/input": "~1.2",
        "joomla/model": "~1.2",
        "joomla/registry": "~1.5",
        "joomla/renderer": "2.*@dev",
        "joomla/router": "~1.1",
        "joomla/uri": "~1.1",
        "joomla/view": "2.*@dev",
        "doctrine/cache": "~1.6",
        "league/flysystem": "~1.0",
        "monolog/monolog": "~1.17",
        "ramsey/uuid": "~3.4",
        "theiconic/php-ga-measurement-protocol": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4",
        "joomla/session": "~1.3"
    },
    "autoload": {
        "psr-4": {
            "Stats\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stats\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "5.6.23"
        }
    }
}
