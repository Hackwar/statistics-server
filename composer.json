{
    "name": "joomla/jstats-server",
    "type": "library",
    "description": "Joomla Stats Collection Server",
    "keywords": ["joomla"],
    "homepage": "http://github.com/dongilbert/jstats",
    "license": "GPL",
    "authors": [
        {
            "name": "Don Gilbert",
            "email": "don@dongilbert.net",
            "homepage": "http://dongilbert.net/"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "joomla/application": "~1.4",
        "joomla/controller": "~1.1",
        "joomla/database": "~1.2",
        "joomla/di": "~1.3",
        "joomla/input": "~1.2",
        "joomla/model": "~1.2",
        "joomla/registry": "~1.4 >=1.4.5",
        "joomla/renderer": "2.*@dev",
        "joomla/router": "~1.1",
        "joomla/uri": "~1.1",
        "joomla/view": "2.*@dev",
        "monolog/monolog": "~1.17"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "joomla/session": "~1.3"
    },
    "autoload": {
        "psr-4": {
            "Stats\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stats\\Tests\\": "tests/"
        }
    }
}
