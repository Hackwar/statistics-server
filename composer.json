{
    "name": "joomla/statistics-server",
    "type": "project",
    "description": "Joomla Stats Collection Server",
    "keywords": ["joomla"],
    "homepage": "http://github.com/joomla/statistics-server",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "^7.2.5",
        "ext-json": "*",
        "ext-pdo": "*",
        "joomla/application": "^2.0@beta",
        "joomla/console": "^2.0@beta",
        "joomla/controller": "^2.0@beta",
        "joomla/database": "^2.0@beta",
        "joomla/event": "^2.0@beta",
        "joomla/di": "^2.0@beta",
        "joomla/filter": "^2.0@beta",
        "joomla/github": "^2.0-dev",
        "joomla/http": "^2.0@beta",
        "joomla/input": "^2.0@beta",
        "joomla/registry": "^2.0@beta",
        "joomla/router": "^2.0@beta",
        "joomla/string": "^2.0@beta",
        "joomla/uri": "^2.0@beta",
        "joomla/utilities": "^2.0@beta",
        "joomla/view": "^2.0@beta",
        "league/flysystem": "^1.0.69",
        "monolog/monolog": "^2.1",
        "psr/log": "^1.1.3",
        "ramsey/uuid": "^3.9.2",
        "symfony/process": "^5.1",
        "theiconic/php-ga-measurement-protocol": "^2.7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16.3",
        "joomla/coding-standards": "dev-master",
        "joomla/test": "^2.0@beta",
        "league/flysystem-memory": "^1.0.2",
        "phpunit/phpunit": "^8.5.5"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\StatsServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\StatsServer\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-php55": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php72": "*"
    },
    "config": {
        "platform": {
            "php": "7.2.5"
        }
    }
}
